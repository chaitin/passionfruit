{
  "name": "passionfruit",
  "version": "0.1.8",
  "description": "Inject to iOS App",
  "main": "app.js",
  "engines": {
    "node": ">= 7.10.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "es2015",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            "transform-runtime"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "prepare": "pushd gui && npm install && popd && npm run compile",
    "prepublishOnly": "cd gui && npm run build",
    "dev": "./scripts/dev-all.js",
    "compile": "./scripts/compile.js compile",
    "watch": "./scripts/compile.js watch",
    "start": "cross-env NODE_ENV=development nodemon ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chaitin/passionfruit.git"
  },
  "keywords": [
    "iOS",
    "frida"
  ],
  "bin": "bin/cli.js",
  "files": [
    "gui/index.html",
    "gui/static",
    "lib/",
    "bin/",
    "agent/*.bundle.js"
  ],
  "author": "CodeColorist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chaitin/passionfruit/issues"
  },
  "homepage": "https://github.com/chaitin/passionfruit#readme",
  "dependencies": {
    "base64-js": "^1.2.1",
    "frida": "^10.6.29",
    "frida-compile": "^5.2.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.3.0",
    "koa-send": "^4.1.2",
    "plist": "^2.1.0",
    "promise.prototype.finally": "^3.1.0",
    "socket.io": "^2.0.4",
    "socket.io-stream": "^0.9.1",
    "sqlite3": "^3.1.13",
    "colors": "^1.1.2"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babelify": "^8.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "macho": "^1.2.1",
    "nodemon": "^1.14.6"
  }
}
