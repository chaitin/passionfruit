{
  "name": "passionfruit",
  "version": "0.2.1",
  "description": "Inject to iOS App",
  "main": "app.js",
  "engines": {
    "node": ">= 7.10.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "es2015",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            "transform-runtime"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "prepare": "pushd gui && npm install && popd && npm run compile",
    "prepublishOnly": "cd gui && npm run build",
    "lint": "npm run lint:agent && npm run lint:server && npm run lint:frontend",
    "lint:agent": "eslint agent/app",
    "lint:server": "eslint *.js && eslint lib",
    "lint:frontend": "eslint gui/src",
    "dev": "./scripts/dev-all.js",
    "compile": "./scripts/compile.js compile",
    "watch": "./scripts/compile.js watch",
    "start": "cross-env NODE_ENV=development nodemon ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chaitin/passionfruit.git"
  },
  "keywords": [
    "iOS",
    "frida"
  ],
  "bin": "bin/cli.js",
  "files": [
    "gui/index.html",
    "gui/static",
    "lib/",
    "bin/",
    "agent/*.bundle.js",
    "agent/*.bundle.bin"
  ],
  "author": "CodeColorist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chaitin/passionfruit/issues"
  },
  "homepage": "https://github.com/chaitin/passionfruit#readme",
  "dependencies": {
    "colors": "^1.2.1",
    "frida": "^10.7.0",
    "frida-compile": "^5.3.6",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "plist": "^2.1.0",
    "promise.prototype.finally": "^3.1.0",
    "socket.io": "^2.0.4",
    "socket.io-stream": "^0.9.1",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babelify": "^8.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-vue": "^4.3.0",
    "fatmacho": "^0.1.1",
    "macho": "^1.2.1",
    "nodemon": "^1.17.2"
  }
}
